This is the dasboard page.  It should be personal to each person.

<% if @auth %>
  <div>Welcome <%= @auth[:username] %></div>

  <div>Photo: <%#= image_tag @auth.photos.where(:is_primary => true).first.image.thumb.url %></div>
  <div>Email: <%= @auth[:email] %></div>
  <div>Age <%= @auth[:age] %></div>
  <div>Gender <%= @auth[:gender] %></div>
  <div>City <%= @auth[:city] %></div>
  <div>State: <%= @auth[:state] %></div>
  <div>Description: <%= @auth[:description] %></div>

  <div>Messages:</div>
  <% if @auth.messages.any? %>
    <% @auth.messages.each do |message| %>
      <div><%= link_to message.subject, message_path(message) %></div>
    <% end %>
  <% else %>
    <div>You have no new messages<div>
  <% end %>
  <div><%= link_to 'Send a new message', new_message_path %></div>
  <div><%= link_to 'Search for new dates', search_path %></div>

  <div class="photo1">
    Add photos to your profile!
    <%= link_to "New Photo", new_photo_path %>
  </div>

  <div>Search for new dates</div>

<% else %>

  <div>Not logged in</div>

<% end %>